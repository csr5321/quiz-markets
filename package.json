{
    "name": "quiz-markets",
    "version": "1.0.0",
    "private": true,
    "scripts": {
      "start": "node server/server.js",
      "server": "nodemon server/server.js",
      "client": "cd client && npm start",
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "install-all": "npm install && cd client && npm install && cd ../server && npm install"
    },
    "devDependencies": {
      "concurrently": "^8.2.2",
      "nodemon": "^3.0.2"
    }
  }
  